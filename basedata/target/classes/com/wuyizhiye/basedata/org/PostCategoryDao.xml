<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuyizhiye.basedata.org.dao.PostCategoryDao">
<!-- 获得树节点 -->
<select id="getSimpleTreeData" parameterType="map" resultType="com.wuyizhiye.basedata.org.model.PostCategory">
	SELECT
		D.FID AS "id",
		D.FNAME AS "name",
		D.FNUMBER AS "number",
		D.FLONGNUMBER AS "longNumber"
	FROM T_BD_POSTCATEGORY D
</select>
</mapper>